    :hasSemanticRole "Conflict Prevention Principle" ;
    :hasSocialSignification "Legal Safety Obligation" ;
    :hasPhomenologicalAspect "Safety confidence foundation" ;
    :states "Conflicting signals must never be active simultaneously" ;
    :appliesTo :traffic_light_assemblage ;
    :manifestsAs :never_both_traffic_and_pedestrian_green .

:SAFE_AX002 a :MaterialAxiom ;
    rdfs:label "Progressive Warning Axiom" ;
    :hasOntologicalStatus "Ethical Safety Principle" ;
    :hasSemanticRole "Fair Warning Provision" ;
    :hasSocialSignification "Driver Rights Protection" ;
    :hasPhomenologicalAspect "Predictable transition security" ;
    :states "State transitions must provide adequate warning to affected parties" ;
    :appliesTo :amber_traffic_led ;
