    :manifestsAs :finite_state_automaton_behavior .

:COMP_AX004 a :ComputationalAxiom ;
    rdfs:label "Non-Blocking Execution Axiom" ;
    :hasOntologicalStatus "Real-Time System Principle" ;
    :hasSemanticRole "Responsive System Behavior" ;
    :hasSocialSignification "User Experience Quality Foundation" ;
    :hasPhomenologicalAspect "Immediate system responsiveness expectation" ;
    :states "System main loop must not block on long-duration operations" ;
    :appliesTo :arduino_main_loop ;
    :manifestsAs :millis_based_timing_instead_of_delay .

:COMP_AX005 a :ComputationalAxiom ;
    rdfs:label "Digital Abstraction Axiom" ;
    :hasOntologicalStatus "Interface Simplification Principle" ;
    :hasSemanticRole "Complexity Management" ;
    :hasSocialSignification "Engineering Abstraction Foundation" ;
    :hasPhomenologicalAspect "Simplified interface hiding underlying complexity" ;
    :states "Higher-level abstractions hide lower-level implementation details" ;
    :appliesTo :c_programming_language,
              :arduino_api ;
    :manifestsAs :digitalWrite_abstraction_over_register_manipulation .

