    :hasOntologicalStatus "Logical Foundation Analysis" ;
    :hasSemanticRole "Axiom Set Validation" ;
    :hasSocialSignification "Intellectual Rigor in System Design" ;
    :hasPhomenologicalAspect "Confidence in logical foundation" ;
