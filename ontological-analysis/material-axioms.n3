# === FUNDAMENTAL MATERIAL AXIOMS ===

:MaterialAxiom a owl:Class ;
    rdfs:subClassOf :FormalBeing ;
    rdfs:label "Material Axiom" ;
    rdfs:comment "Foundational truth about material reality" .

:OntologicalAxiom a owl:Class ;
    rdfs:subClassOf :FormalBeing ;
    rdfs:label "Ontological Axiom" ;
    rdfs:comment "Foundational truth about being and existence" .

:PhysicalAxiom a owl:Class ;
    rdfs:subClassOf :MaterialAxiom ;
    rdfs:label "Physical Axiom" ;
    rdfs:comment "Foundational truth about physical reality" .

:ComputationalAxiom a owl:Class ;
    rdfs:subClassOf :MaterialAxiom ;
    rdfs:label "Computational Axiom" ;
    rdfs:comment "Foundational truth about computational systems" .

:TemporalAxiom a owl:Class ;
    rdfs:subClassOf :OntologicalAxiom ;
    rdfs:label "Temporal Axiom" ;
    rdfs:comment "Foundational truth about time and temporality" .

:SimulationAxiom a owl:Class ;
    rdfs:subClassOf :OntologicalAxiom ;
    rdfs:label "Simulation Axiom" ;
    rdfs:comment "Foundational truth about simulation and virtual reality" .

# === PHYSICAL REALITY AXIOMS ===

:PHYS_AX001 a :PhysicalAxiom ;
    rdfs:label "Conservation of Energy Axiom" ;
    :hasOntologicalStatus "Universal Physical Law" ;
    :hasSemanticRole "Energy Conservation Principle" ;
    :hasSocialSignification "Foundation of Engineering Reliability" ;
    :hasPhomenologicalAspect "Background assumption for all physical calculations" ;
    :states "Energy cannot be created or destroyed, only transformed" ;
    :appliesTo :arduino_instance,
              :red_traffic_led,
              :amber_traffic_led,
              :green_traffic_led,
              :pedestrian_red_led,
              :pedestrian_green_led ;
    :manifestsAs :electrical_energy_to_light_conversion,
                :heat_dissipation_in_resistors .

:PHYS_AX002 a :PhysicalAxiom ;
    rdfs:label "Ohm's Law Axiom" ;
    :hasOntologicalStatus "Fundamental Electrical Law" ;
    :hasSemanticRole "Electrical Relationship Definition" ;
    :hasSocialSignification "Foundation of Electrical Engineering" ;
    :hasPhomenologicalAspect "Invisible electrical relationships governing circuit behavior" ;
    :states "V = IR - Voltage equals Current times Resistance" ;
    :appliesTo :resistor_220ohm_red,
              :resistor_220ohm_amber,
              :resistor_220ohm_green,
              :resistor_220ohm_ped_red,
              :resistor_220ohm_ped_green ;
    :manifestsAs :current_limitation_through_resistance .

:PHYS_AX003 a :PhysicalAxiom ;
    rdfs:label "Semiconductor Physics Axiom" ;
    :hasOntologicalStatus "Quantum-Level Physical Law" ;
    :hasSemanticRole "Light Emission Mechanism" ;
    :hasSocialSignification "Foundation of LED Technology" ;
    :hasPhomenologicalAspect "Invisible quantum processes enabling visible light" ;
    :states "Electron-hole recombination in semiconductor junction produces photons" ;
    :appliesTo :red_traffic_led,
              :amber_traffic_led,
              :green_traffic_led,
              :pedestrian_red_led,
              :pedestrian_green_led ;
    :manifestsAs :wavelength_specific_photon_emission .

:PHYS_AX004 a :PhysicalAxiom ;
    rdfs:label "Digital Logic Axiom" ;
    :hasOntologicalStatus "Binary State Foundation" ;
    :hasSemanticRole "Computational State Definition" ;
    :hasSocialSignification "Foundation of Digital Technology" ;
    :hasPhomenologicalAspect "Binary simplification of analog reality" ;
    :states "Digital signals exist in discrete HIGH (5V) and LOW (0V) states" ;
    :appliesTo :pin13, :pin12, :pin11, :pin10, :pin9, :pin8 ;
    :manifestsAs :voltage_threshold_switching .

:PHYS_AX005 a :PhysicalAxiom ;
    rdfs:label "Clock Crystal Oscillation Axiom" ;
    :hasOntologicalStatus "Temporal Reference Standard" ;
    :hasSemanticRole "System Temporal Synchronization" ;
    :hasSocialSignification "Foundation of Digital Timing" ;
    :hasPhomenologicalAspect "Invisible temporal regularity infrastructure" ;
    :states "Quartz crystal oscillates at precise 16MHz frequency" ;
    :appliesTo :arduino_instance ;
    :manifestsAs :arduino_clock_cycle .

# === COMPUTATIONAL REALITY AXIOMS ===

:COMP_AX001 a :ComputationalAxiom ;
    rdfs:label "Church-Turing Thesis Axiom" ;
    :hasOntologicalStatus "Fundamental Computability Principle" ;
    :hasSemanticRole "Computational Equivalence Foundation" ;
    :hasSocialSignification "Theoretical Computer Science Foundation" ;
    :hasPhomenologicalAspect "Abstract computational universality" ;
    :states "Any effectively calculable function can be computed by a Turing machine" ;
    :appliesTo :arduino_instance ;
    :manifestsAs :traffic_control_algorithm_computability .

:COMP_AX002 a :ComputationalAxiom ;
    rdfs:label "Von Neumann Architecture Axiom" ;
    :hasOntologicalStatus "Stored Program Principle" ;
    :hasSemanticRole "Program-Data Unification" ;
    :hasSocialSignification "Foundation of Modern Computing" ;
    :hasPhomenologicalAspect "Flexible programmability assumption" ;
    :states "Instructions and data are stored in the same memory space" ;
    :appliesTo :arduino_instance ;
    :manifestsAs :c_code_compilation_to_machine_instructions .

:COMP_AX003 a :ComputationalAxiom ;
    rdfs:label "Deterministic State Machine Axiom" ;
    :hasOntologicalStatus "Computational Predictability Principle" ;
    :hasSemanticRole "System Behavior Determinism" ;
    :hasSocialSignification "Foundation of System Reliability" ;
    :hasPhomenologicalAspect "Predictable system behavior expectation" ;
    :states "Given identical inputs and state, system produces identical outputs" ;
