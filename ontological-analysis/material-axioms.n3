# === AXIOM RELATIONSHIPS ===

:states a owl:DatatypeProperty ;
    rdfs:label "states" ;
    rdfs:comment "Textual statement of the axiom" ;
    rdfs:domain :MaterialAxiom ;
    rdfs:range xsd:string .

:appliesTo a owl:ObjectProperty ;
    rdfs:label "applies to" ;
    rdfs:comment "Axiom applies to specific system components or aspects" ;
    rdfs:domain :MaterialAxiom ;
    rdfs:range :Being .

:manifestsAs a owl:ObjectProperty ;
    rdfs:label "manifests as" ;
    rdfs:comment "How the axiom manifests in the concrete system" ;
    rdfs:domain :MaterialAxiom ;
    rdfs:range :Being .

:foundationFor a owl:ObjectProperty ;
    rdfs:label "foundation for" ;
    rdfs:comment "Axiom provides foundation for higher-level properties" ;
    rdfs:domain :MaterialAxiom ;
    rdfs:range :Being .

:constrains a owl:ObjectProperty ;
    rdfs:label "constrains" ;
    rdfs:comment "Axiom provides constraints on system behavior" ;
    rdfs:domain :MaterialAxiom ;
    rdfs:range :Being .

