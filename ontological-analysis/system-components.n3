    :hasOntologicalStatus "Protective Material Mediator" ;
    :hasResistance "220"^^xsd:decimal ;
    :hasSemanticRole "Current Limitation and Component Protection" ;
    :hasSocialSignification "Engineering Ethics - Harm Prevention Through Design" ;
    :hasPhomenologicalAspect "Invisible infrastructure - withdrawn protective function" ;
    :assemblesWith :green_traffic_led .

:resistor_220ohm_ped_red a :Resistor220Ohm ;
    rdfs:label "220立 Resistor for Pedestrian Red LED" ;
    :hasOntologicalStatus "Protective Material Mediator" ;
    :hasResistance "220"^^xsd:decimal ;
    :hasSemanticRole "Current Limitation and Component Protection" ;
    :hasSocialSignification "Engineering Ethics - Harm Prevention Through Design" ;
    :hasPhomenologicalAspect "Invisible infrastructure - withdrawn protective function" ;
    :assemblesWith :pedestrian_red_led .

:resistor_220ohm_ped_green a :Resistor220Ohm ;
    rdfs:label "220立 Resistor for Pedestrian Green LED" ;
    :hasOntologicalStatus "Protective Material Mediator" ;
    :hasResistance "220"^^xsd:decimal ;
    :hasSemanticRole "Current Limitation and Component Protection" ;
    :hasSocialSignification "Engineering Ethics - Harm Prevention Through Design" ;
    :hasPhomenologicalAspect "Invisible infrastructure - withdrawn protective function" ;
    :assemblesWith :pedestrian_green_led .

# === BUTTON ONTOLOGICAL SPECIFICATION ===

:PedestrianButton a owl:Class ;
    rdfs:subClassOf :PushButton ;
    rdfs:label "Pedestrian Button" ;
    rdfs:comment "Momentary switch for pedestrian crossing requests" .

:pedestrian_button a :PedestrianButton ;
    rdfs:label "Pedestrian Button Instance" ;
    :hasOntologicalStatus "Human-Machine Interface Materializer" ;
    :hasSemanticRole "Agency Translation - Human Intention to Computational Event" ;
    :hasSocialSignification "Democratic Participation Interface - Citizen Voice in Infrastructure" ;
    :hasPhomenologicalAspect "Extension of bodily agency into technological assemblage" ;
    :prehends :human_walking_intentions ;
    :actualizes :pedestrian_crossing_requests ;
    :assemblesWith :pin13 .

# === BREADBOARD AND WIRING ONTOLOGICAL SPECIFICATION ===

:Breadboard a owl:Class ;
    rdfs:subClassOf :ElectronicComponent ;
    rdfs:label "Breadboard" ;
    rdfs:comment "Temporary circuit construction platform" .

:breadboard_instance a :Breadboard ;
    rdfs:label "Prototyping Breadboard" ;
    :hasOntologicalStatus "Experimental Infrastructure" ;
    :hasSemanticRole "Temporary Material Organization Platform" ;
    :hasSocialSignification "Educational and Experimental Space" ;
    :hasPhomenologicalAspect "Workshop ready-to-hand equipment" ;
    :assemblesWith :arduino_instance .

:JumperWire a owl:Class ;
    rdfs:subClassOf :ElectronicComponent ;
    rdfs:label "Jumper Wire" ;
    rdfs:comment "Connecting wire for breadboard circuits" .

:ground_rail a :JumperWire ;
    rdfs:label "Ground Rail Connection" ;
    :hasOntologicalStatus "Electrical Reference Point Materializer" ;
    :hasSemanticRole "Common Electrical Reference" ;
    :hasSocialSignification "Shared Foundation - Democratic Electrical Equality" ;
    :hasPhomenologicalAspect "Invisible background condition for electrical function" .

:power_rail a :JumperWire ;
    rdfs:label "Power Rail Connection" ;
    :hasOntologicalStatus "Energy Distribution Infrastructure" ;
    :hasSemanticRole "Electrical Energy Supply" ;
    :hasSocialSignification "Life-giving Infrastructure - System Vitality" ;
    :hasPhomenologicalAspect "Invisible energy flow - background condition for operation" .

# === COMPONENT ASSEMBLAGES ===

:TrafficLightAssemblage a owl:Class ;
    rdfs:subClassOf :ComputationalSystem ;
    rdfs:label "Traffic Light Assemblage" ;
    rdfs:comment "Complete heterogeneous assemblage of all system components" .

:traffic_light_assemblage a :TrafficLightAssemblage ;
    rdfs:label "Complete Traffic Light System Assemblage" ;
    :hasOntologicalStatus "Heterogeneous Machinic Assemblage" ;
    :hasSemanticRole "Urban Traffic Coordination Infrastructure" ;
    :hasSocialSignification "Socio-Technical Network for Urban Mobility" ;
    :hasPhomenologicalAspect "Urban infrastructure - oscillates between transparent and thematic" ;
    :assemblesWith :arduino_instance,
                  :red_traffic_led,
                  :amber_traffic_led,
                  :green_traffic_led,
                  :pedestrian_red_led,
                  :pedestrian_green_led,
                  :pedestrian_button,
                  :breadboard_instance ;
    :embodies :traffic_coordination_logic ;
    :actualizes :urban_mobility_patterns .

# === COMPONENT RELATIONSHIPS ===

# Electrical Connections
:pin13 :electricallyConnectedTo :pedestrian_button .
:pin12 :electricallyConnectedTo :resistor_220ohm_red .
:pin11 :electricallyConnectedTo :resistor_220ohm_amber .
:pin10 :electricallyConnectedTo :resistor_220ohm_green .
:pin9 :electricallyConnectedTo :resistor_220ohm_ped_red .
:pin8 :electricallyConnectedTo :resistor_220ohm_ped_green .

:resistor_220ohm_red :electricallyConnectedTo :red_traffic_led .
:resistor_220ohm_amber :electricallyConnectedTo :amber_traffic_led .
:resistor_220ohm_green :electricallyConnectedTo :green_traffic_led .
:resistor_220ohm_ped_red :electricallyConnectedTo :pedestrian_red_led .
:resistor_220ohm_ped_green :electricallyConnectedTo :pedestrian_green_led .

# Ontological Relationships
:arduino_instance :embodies :computational_logic .
:red_traffic_led :materializes :prohibition_concept .
:amber_traffic_led :materializes :transition_concept .
:green_traffic_led :materializes :permission_concept .
:pedestrian_red_led :materializes :pedestrian_constraint_concept .
:pedestrian_green_led :materializes :pedestrian_empowerment_concept .
:pedestrian_button :transduces :human_agency_into_computational_events .

