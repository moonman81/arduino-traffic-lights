    rdfs:label "220 Ohm Resistor" ;
    rdfs:comment "Current-limiting resistor for LED protection" .

:resistor_220ohm_red a :Resistor220Ohm ;
    rdfs:label "220立 Resistor for Red Traffic LED" ;
    :hasOntologicalStatus "Protective Material Mediator" ;
    :hasResistance "220"^^xsd:decimal ;
    :hasSemanticRole "Current Limitation and Component Protection" ;
    :hasSocialSignification "Engineering Ethics - Harm Prevention Through Design" ;
    :hasPhomenologicalAspect "Invisible infrastructure - withdrawn protective function" ;
    :assemblesWith :red_traffic_led .

:resistor_220ohm_amber a :Resistor220Ohm ;
    rdfs:label "220立 Resistor for Amber Traffic LED" ;
    :hasOntologicalStatus "Protective Material Mediator" ;
    :hasResistance "220"^^xsd:decimal ;
    :hasSemanticRole "Current Limitation and Component Protection" ;
    :hasSocialSignification "Engineering Ethics - Harm Prevention Through Design" ;
    :hasPhomenologicalAspect "Invisible infrastructure - withdrawn protective function" ;
    :assemblesWith :amber_traffic_led .

:resistor_220ohm_green a :Resistor220Ohm ;
