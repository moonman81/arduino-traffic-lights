# System Components Ontological Specification
# Ultra-Detailed Component Analysis
# Document: system-components.n3
# Version: 1.0
# Purpose: Detailed ontological description of all system components

@prefix : <http://example.org/traffic-light-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Include base ontology
@include "base-ontology.n3" .

# === ARDUINO UNO R3 ONTOLOGICAL SPECIFICATION ===

:ArduinoUnoR3 a owl:Class ;
    rdfs:subClassOf :Microcontroller ;
    rdfs:label "Arduino Uno R3" ;
    rdfs:comment "Specific microcontroller board with ATmega328P processor" .

:arduino_instance a :ArduinoUnoR3 ;
    rdfs:label "Traffic Light Arduino Instance" ;
    :hasOntologicalStatus "Primary Material Substrate" ;
    :hasClockFrequency "16000000"^^xsd:decimal ;
    :hasSemanticRole "Central Processing Unit for Traffic Control Logic" ;
    :hasSocialSignification "Educational Tool for Embedded Systems Learning" ;
    :hasPhomenologicalAspect "Ready-to-hand equipment that withdraws during normal operation" ;
    :embodies :traffic_control_algorithm ;
    :actualizes :potential_state_space .

# Arduino Pins as Ontological Entities
:DigitalPin a owl:Class ;
    rdfs:subClassOf :ElectronicComponent ;
    rdfs:label "Digital Pin" ;
    rdfs:comment "Input/output interface point on microcontroller" .

:pin13 a :DigitalPin ;
    rdfs:label "Pin 13 - Pedestrian Button Input" ;
    :hasOntologicalStatus "Interface Between Human Agency and Computational Logic" ;
    :hasSemanticRole "Pedestrian Intention Transduction" ;
    :hasSocialSignification "Democratic Participation in Urban Infrastructure" ;
    :hasPhomenologicalAspect "Extension of pedestrian bodily schema into computational realm" ;
    :hasVoltage "5.0"^^xsd:decimal ;
    :prehends :pedestrian_intentions .

:pin12 a :DigitalPin ;
    rdfs:label "Pin 12 - Red Traffic LED" ;
    :hasOntologicalStatus "Material Signifier of Traffic Prohibition" ;
    :hasSemanticRole "STOP Command Materialization" ;
    :hasSocialSignification "Authority and Legal Constraint Embodiment" ;
    :hasPhomenologicalAspect "Immediate perceptual imperative - pre-reflective halt response" ;
    :embodies :prohibition_essence .

:pin11 a :DigitalPin ;
    rdfs:label "Pin 11 - Amber Traffic LED" ;
    :hasOntologicalStatus "Material Signifier of Temporal Transition" ;
    :hasSemanticRole "Prepare-to-Stop Command Materialization" ;
    :hasSocialSignification "Temporal Grace Period - Ethical Consideration for In-Motion Traffic" ;
    :hasPhomenologicalAspect "Anxiety-inducing ambiguity - decision point pressure" ;
    :embodies :transition_essence .

:pin10 a :DigitalPin ;
    rdfs:label "Pin 10 - Green Traffic LED" ;
    :hasOntologicalStatus "Material Signifier of Traffic Permission" ;
    :hasSemanticRole "GO Command Materialization" ;
    :hasSocialSignification "Freedom of Movement Authorization" ;
    :hasPhomenologicalAspect "Relief and flow-state enabling - transparent operation" ;
    :embodies :permission_essence .

:pin9 a :DigitalPin ;
    rdfs:label "Pin 9 - Pedestrian Red LED" ;
    :hasOntologicalStatus "Material Signifier of Pedestrian Constraint" ;
    :hasSemanticRole "Pedestrian STOP Command Materialization" ;
    :hasSocialSignification "Pedestrian-Specific Authority Assertion" ;
    :hasPhomenologicalAspect "Frustration and temporal suspension experience" ;
    :embodies :pedestrian_prohibition_essence .

:pin8 a :DigitalPin ;
    rdfs:label "Pin 8 - Pedestrian Green LED" ;
    :hasOntologicalStatus "Material Signifier of Pedestrian Permission" ;
    :hasSemanticRole "Pedestrian GO Command Materialization" ;
    :hasSocialSignification "Pedestrian Safety and Priority Recognition" ;
    :hasPhomenologicalAspect "Relief and urgency - limited time window awareness" ;
    :embodies :pedestrian_permission_essence .

# === LED ONTOLOGICAL SPECIFICATIONS ===

:RedTrafficLED a owl:Class ;
    rdfs:subClassOf :LightEmittingDiode ;
    rdfs:label "Red Traffic LED" ;
    rdfs:comment "Red light emitting diode for traffic stop signal" .

:red_traffic_led a :RedTrafficLED ;
    rdfs:label "Red Traffic LED Instance" ;
    :hasOntologicalStatus "Semiotic-Material Hybrid" ;
    :hasColorProperty "Red (approximately 630-700nm wavelength)" ;
    :hasElectricalCurrent "0.0136"^^xsd:decimal ;
    :hasVoltage "2.0"^^xsd:decimal ;
    :hasSemanticRole "Universal Stop Signifier" ;
    :hasSocialSignification "Culturally Learned Prohibition Response" ;
    :hasPhomenologicalAspect "Immediate visceral recognition - pre-cognitive response" ;
    :assemblesWith :resistor_220ohm_red ;
    :embodies :redness_as_prohibition .

:AmberTrafficLED a owl:Class ;
    rdfs:subClassOf :LightEmittingDiode ;
    rdfs:label "Amber Traffic LED" ;
    rdfs:comment "Amber/yellow light emitting diode for traffic caution signal" .

:amber_traffic_led a :AmberTrafficLED ;
    rdfs:label "Amber Traffic LED Instance" ;
    :hasOntologicalStatus "Temporal-Transition Materializer" ;
    :hasColorProperty "Amber/Yellow (approximately 570-590nm wavelength)" ;
    :hasElectricalCurrent "0.0132"^^xsd:decimal ;
    :hasVoltage "2.1"^^xsd:decimal ;
    :hasSemanticRole "Temporal Warning and Transition Signifier" ;
    :hasSocialSignification "Ethical Consideration for Driver Decision-Making" ;
    :hasPhomenologicalAspect "Anxiety-inducing uncertainty - temporal pressure" ;
    :assemblesWith :resistor_220ohm_amber ;
    :embodies :yellowness_as_temporal_tension .

:GreenTrafficLED a owl:Class ;
    rdfs:subClassOf :LightEmittingDiode ;
    rdfs:label "Green Traffic LED" ;
    rdfs:comment "Green light emitting diode for traffic go signal" .

:green_traffic_led a :GreenTrafficLED ;
    rdfs:label "Green Traffic LED Instance" ;
    :hasOntologicalStatus "Permission-Granting Material Entity" ;
    :hasColorProperty "Green (approximately 495-570nm wavelength)" ;
    :hasElectricalCurrent "0.0127"^^xsd:decimal ;
    :hasVoltage "2.2"^^xsd:decimal ;
    :hasSemanticRole "Movement Permission Signifier" ;
    :hasSocialSignification "Social Coordination Through Synchronized Permission" ;
    :hasPhomenologicalAspect "Relief and flow-enabling - background condition for smooth movement" ;
    :assemblesWith :resistor_220ohm_green ;
    :embodies :greenness_as_permission .

:PedestrianRedLED a owl:Class ;
    rdfs:subClassOf :LightEmittingDiode ;
    rdfs:label "Pedestrian Red LED" ;
    rdfs:comment "Red LED specifically for pedestrian stop signal" .

:pedestrian_red_led a :PedestrianRedLED ;
    rdfs:label "Pedestrian Red LED Instance" ;
    :hasOntologicalStatus "Pedestrian-Specific Authority Materializer" ;
    :hasColorProperty "Red (approximately 630-700nm wavelength)" ;
    :hasElectricalCurrent "0.0136"^^xsd:decimal ;
    :hasVoltage "2.0"^^xsd:decimal ;
    :hasSemanticRole "Pedestrian-Specific Prohibition Signifier" ;
    :hasSocialSignification "Recognition of Pedestrian as Distinct Traffic Category" ;
    :hasPhomenologicalAspect "Frustration and enforced patience - temporal suspension" ;
    :assemblesWith :resistor_220ohm_ped_red ;
    :embodies :pedestrian_specific_constraint .

:PedestrianGreenLED a owl:Class ;
    rdfs:subClassOf :LightEmittingDiode ;
    rdfs:label "Pedestrian Green LED" ;
    rdfs:comment "Green/white LED for pedestrian walk signal" .

:pedestrian_green_led a :PedestrianGreenLED ;
    rdfs:label "Pedestrian Green LED Instance" ;
    :hasOntologicalStatus "Pedestrian-Priority Materializer" ;
    :hasColorProperty "White/Blue (approximately 450-495nm wavelength)" ;
    :hasElectricalCurrent "0.0091"^^xsd:decimal ;
    :hasVoltage "3.0"^^xsd:decimal ;
    :hasSemanticRole "Pedestrian Permission and Priority Signifier" ;
    :hasSocialSignification "Democratic Participation in Urban Infrastructure" ;
    :hasPhomenologicalAspect "Relief mixed with urgency - limited time window awareness" ;
    :assemblesWith :resistor_220ohm_ped_green ;
    :embodies :pedestrian_empowerment .

# === RESISTOR ONTOLOGICAL SPECIFICATIONS ===

:Resistor220Ohm a owl:Class ;
